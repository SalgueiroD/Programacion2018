#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;
void main (){

	int n ,numeros[10],n, menor=0;
   
	
	cout<<"ingrese el numero del elemento del vector"<<endl;
	cin>>n;


		for(int i= 0;i<n;i++){
        cout<<i+1<<"digite un numero :";
		cin>>numeros [i];
		if(numeros [i] <menor){ 
		 menor =numeros [i];
		}
      }
	     
	
	cout<<"el menor es  :"<<menor<<endl;

	getch ();


}
